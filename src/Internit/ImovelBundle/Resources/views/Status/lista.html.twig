{% extends 'TupiAdminBundle:Template:admin-lista.html.twig' %}

{% set title = 'Status' %}
{% set actions = {'imovel_status_new': {'label': 'Adicionar', 'attrs': {'class': 'adicionar'} } } %}

{% block table_head %}
	<th class="titulo">Status</th>
{% endblock %}

{% block table_body %}
	<td>
		<a href="{{ path('imovel_status_edit', {'id':item.id}) }}">
			<span>{{item.status}}</span>
		</a>
	</td>
{% endblock %}